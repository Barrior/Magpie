{
  "name": "Magpie",
  "version": "1.0.0",
  "description": "A validated JavaScript RegExp library.",
  "scripts": {
    "lint": "eslint --ext .js src,test",
    "test": "ava --verbose",
    "build": "npm run lint && npm test && node ./bin/build"
  },
  "ava": {
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit",
    "files": [
      "./test/*.js"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-core": "^6.22.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "chalk": "^2.3.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "js-beautify": "^1.7.5",
    "shelljs": "^0.8.1",
    "uglify-js": "^3.3.12"
  },
  "license": "MIT"
}
